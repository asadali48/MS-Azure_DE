{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Stage1-cc2"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"adls3rdconn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls3rdconn'"
		},
		"myADLS1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'myADLS1'"
		},
		"adls3rdconn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage3346.dfs.core.windows.net/"
		},
		"myADLS1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage3346.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Stage1-cc2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"adls3rdconn_accountKey": {
						"value": "[parameters('adls3rdconn_accountKey')]"
					},
					"myADLS1_accountKey": {
						"value": "[parameters('myADLS1_accountKey')]"
					},
					"adls3rdconn_properties_typeProperties_url": {
						"value": "[parameters('adls3rdconn_properties_typeProperties_url')]"
					},
					"myADLS1_properties_typeProperties_url": {
						"value": "[parameters('myADLS1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}